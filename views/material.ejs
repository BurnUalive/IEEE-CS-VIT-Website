            $(active_tab).removeClass('no-view');
           //$(active_tab).fadeIn();
            for (x in a ){
                a[x].onclick= function () {
                    if (!hasClass(this.parentElement,'active')) {
                        var tab_btn = $('.tab-btn');
                        var tab_name;
                        for(var i=0;i<tab_btn.length;i++) {
                            tab_name = tab_btn[i].getAttribute('data-tab-name');
                            tab_btn[i].parentElement.className = '';
                        }
                        var show  = this;
                        tab_name = $(show).data("tab-name");
                        document.body.className = tab_name;
                        $(this.parentElement).addClass('active').addClass(tab_name);
                        $(active_tab).addClass('no-view');
                        active_tab = '#'+tab_name
                        $(active_tab).removeClass('no-view');
                       /* $(active_tab).fadeOut(function(){
                            var tab_name = $(show).data("tab-name");

                            //$(active_tab).fadeIn();
                        });*/
                    }
                };
            }
        }
        function hasClass(element, cls) {
            return (' ' + element.className + ' ').indexOf(' ' + cls + ' ') > -1;
        }
    </script>
    </body>
</html>